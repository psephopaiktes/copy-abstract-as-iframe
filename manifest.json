{
  "name": "copy-abstract-as-iframe",
  "author": "Akira HIRATA",
  "description": "You can copy iframe text for embed Abstract.",
  "version": "1.0.0",
  "manifest_version": 2,
  "web_accessible_resources": ["*"],
  "permissions": ["storage"],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://app.goabstract.com/*"],
      "js": ["script.js"]
    }
  ]
}
